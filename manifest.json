{
  "name": "get employee email",
  "description": "my extension",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
  },
  "background": {
    "scripts": ["./background.js"]
  },
  "options_page": "./options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+I",
        "linux": "Ctrl+I",
        "windows": "Ctrl+I",
        "mac": "Alt+I"
      }
    }
  }
}